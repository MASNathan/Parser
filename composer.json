{
    "name": "masnathan/parser",
    "description": "Global data type parser",
    "keywords": [
        "parser",
        "json",
        "xml",
        "yaml",
        "php",
        "data",
        "encode",
        "decode"
    ],
    "homepage": "https://github.com/masnathan/parser",
    "license": "MIT",
    "authors": [
        {
            "name": "AndrÃ© Filipe",
            "email": "andre.r.flip@gmail.com",
            "homepage": "http://masnathan.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "camspiers/json-pretty": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "symfony/var-dumper": "^2.7",
        "symfony/yaml": "^2.7",
        "nette/neon": "^2.3"
    },
    "suggest": {
        "symfony/yaml": "For parsing YAML files.",
        "nette/neon": "For parsing NEON files."
    },
    "autoload": {
        "psr-4": {
            "MASNathan\\Parser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MASNathan\\Parser\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config":
    {
        "bin-dir": "bin"
    }
}
